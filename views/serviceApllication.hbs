    function checkDtToday(date){
        var diffmonth  = 0;
        var diffyear   = 0;

        //alert(date.substring(0,2));

        var day    = date.substring(0,2);
        var month  = date.substring(3,5);
        var year   = date.substring(6,10);

        chqDate = new Date(date);
        today = new Date();


        chqDate.setMonth(month-1); // January = 0
        chqDate.setFullYear(year);
        chqDate.setHours(0, 0, 0, 0);
