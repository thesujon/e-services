                cache:false,
                dataType: 'json',
                contentType: 'application/json; charset=utf-8',
                success: function (userObj) {

                 window.userObj = userObj;

                 if (userObj){
                     var userKeys = _.keys(userObj);
                     _.each(userKeys, function (key) {
                         var ctl = $("#"+ key);
                         ctl.val(userObj[key]);
