    function calculatePeriod(date, id){

        //var date = sDate.value;
        var day = date.substring(0,2);
        var month = date.substring(3,5);
        var year = date.substring(6,10);

        //Variables contain values of current date
        var today = new Date();
        var dayOfToday = parseInt(today.getDate());
        var monthOfToday = today.getMonth() + 1;
        var yearOfToday = today.getFullYear();

        //Variables required to calculate date.
        var yearDiff;
        var monthDiff;
        var daysDiff;

        var totalNumOfDaysOfMonths = [31,28,31,30,31,30,31,31,30,31,30,31];
        var ageString;


        calcYearDiff();

        function calcYearDiff(){
            if (month > monthOfToday){//tukar
                yearDiff = year - yearOfToday;
                monthDiff = month - monthOfToday;
                calcDateDiff();
            } else if (monthOfToday == month){
                if((day == dayOfToday) || (day > dayOfToday))//tukar
                {
                    yearDiff = year - yearOfToday;
                }else
                    yearDiff = year - yearOfToday - 1;

                calcMonthDiff();

            }else{
                yearDiff = year - yearOfToday - 1;
                monthDiff = 12 - (month - monthOfToday);

                calcDateDiff();
            }
        }

        function calcMonthDiff(){
            if (monthOfToday == month){
                if (day > dayOfToday)//tukar
                    monthDiff = month - monthOfToday;
                else if ( dayOfToday == day )
                    monthDiff = 0;
                else
                    monthDiff = 11;
            }
            calcDateDiff();
        }

        function calcDateDiff(){
            if ( yearOfToday % 4 == 0 )
                totalNumOfDaysOfMonths[1] = 29;

            if (day > dayOfToday)//tukar
                daysDiff = day - dayOfToday;
            else if (dayOfToday == day)
                daysDiff = 0;
            else{
                daysDiff = ( totalNumOfDaysOfMonths[month - 1] - dayOfToday) + dayOfToday;//tukar
            }
        }

        if((month > monthOfToday) && (day < dayOfToday))//tukar
            monthDiff -= 1;

        if(yearDiff == 0)
            ageString = 0;
        else
            ageString = yearDiff;

        if(ageString < 2){
            alert("Expiry date must be more than 2 years");
            document.getElementById(id).focus();
            return false;
        }
        else{
            return true;
        }

    }
